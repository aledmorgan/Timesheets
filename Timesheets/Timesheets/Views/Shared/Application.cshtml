<div ng-view autoscroll="true"></div>

<script type="text/ng-template" id="application/index">
    <div ng-show="loading">
        Loading...
    </div>
    <div ng-show="!loading">
        <div ng-repeat="timesheet in timesheets">
            {{timesheet.Id}}
        </div>
        <div ng-if="timesheets.length === 0">
            No timesheets are available.
        </div>
    </div>
</script>
